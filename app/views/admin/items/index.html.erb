<%= render "items/index" %>
<%= link_to "+", new_admin_item_path %>
<h2>商品一覧</h2>
<% @items.each do |item| %>
  <div class="float3">
    <%= image_tag item.get_image, class: 'item_image' %>
    <h3><%= item.item_name %></h3>
    <p><%= item.item_introduction %></p>
    <p><%= item.price %>円</p>
  </div>
<% end %>
